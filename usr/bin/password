#!/bin/bash

if [ ! "$1" ]; then
echo " ERROR 555483736 - NO ARGUMENT ENTERED";
  exit
fi;

SERV=$1

if [ $? = 1 ]; then
   	echo "No match found";
 else
 	NEWDIR=`ls -dt /var/spool/backup/configs-*/*$SERV* | head -1`;
	IPA=`grep Internal $NEWDIR | cut -c 23-35 | head -1`;	
	VLAN=`grep Phones $NEWDIR | cut -c 21-34`;
if [ ! $VLAN = 0.0.0.0 ]; then
	echo " IP Address is " $IPA " with VLAN IP" $VLAN;
else
	echo "IP Address is " $IPA ", with no VLAN configured." >> scripttesting.txt;
fi	
	grep OPassword $NEWDIR | cut -c 10-;

	UPDATEDT=`echo " . $NEWDIR . " | cut -c 30-39`;
	echo "The last update was" $UPDATEDT;
        SERVERTM=`date +%F`;
	HRS=$((($(date -d "$SERVERTM" +%s)-$(date -d "$UPDATEDT" +%s))/86400))
	echo "Last Update was "$HRS" days ago";
fi
